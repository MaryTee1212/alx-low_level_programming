#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/* more headers goes there*/

/*betty style doc for function main goes there*/
/**
 * main - This program will assign a random munmber to the variable n
 * Return: 0
 */

int main(void)
{
	int n;

	srand(time(0));
	n = rand() -RAND_MAX/2;
	if (n > 0)
	{
		printf9"%d is positive\n", n);
	}
	else if (n == 0)
	{
		printf("%d is zero\n", n);
	return (0);
}
